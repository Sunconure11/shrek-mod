{
  "schemaVersion": 1,
  "id": "shrek",
  "version": "${mod_version}",
  "name": "${mod_name}",
  "description": "${mod_description}",
  "authors": [
    "${mod_author}"
  ],
  "contributors": [
    "${mod_credits}"
  ],
  "contact": {
    "homepage": "${mod_url}",
    "issues": "${mod_issues}",
    "sources": "${mod_sources}"
  },
  "license": "${mod_license}",
  "icon": "${mod_logo}",
  "environment": "*",
  "entrypoints": {
    "main": [
      "net.pixeldreamstudios.shrek.FabricShrekMod"
    ],
    "client": [
      "net.pixeldreamstudios.shrek.ClientListener"
    ]
  },
  "mixins": [
    "${mod_id}.fabric.mixins.json"
  ],
  "depends": {
    "fabricloader": ">=${fabric_loader_version}",
    "fabric-api": "*",
    "java": ">=17",
    "minecraft": "~${minecraft_version}"
  }
}